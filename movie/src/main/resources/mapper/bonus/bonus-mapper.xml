<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.movie.mapper.BonusMapper">
	<select id="getList" resultType="com.example.movie.dto.BonusDto">
		SELECT * FROM BONUS
	</select>
	
	<insert id="insertBonus" parameterType="com.example.movie.dto.BonusDto">
		INSERT INTO BONUS (ename) VALUES (#{ename})
	</insert>
</mapper>




